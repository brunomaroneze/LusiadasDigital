               <ref name="tei_LusiadasdocAuthor"/>
               <ref name="tei_LusiadasdocImprint"/>
               <ref name="tei_LusiadasdocTitle"/>
               <ref name="tei_LusiadastitlePart"/>
               <ref name="tei_Lusiadasmodel.gLike"/>
               <ref name="tei_Lusiadasmodel.phrase"/>
